{
  "name": "admin-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/konstellation-io/kre.git"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.5.1",
    "@types/react-syntax-highlighter": "^11.0.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-upload-client": "^11.0.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "d3-array": "^2.4.0",
    "d3-axis": "^1.0.12",
    "d3-color": "^1.4.0",
    "d3-format": "^1.4.4",
    "d3-interpolate": "^1.4.0",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-zoom": "^1.8.3",
    "enzyme-to-json": "^3.4.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql.macro": "^1.4.2",
    "lodash": "^4.17.15",
    "lottie-web": "^5.5.9",
    "markdown-navbar": "^1.4.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dates": "^21.5.0",
    "react-dom": "^16.9.0",
    "react-grid-resizable": "^1.0.0",
    "react-hook-form": "^5.0.3",
    "react-hotkeys": "^2.0.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.1.0",
    "react-scripts": "3.1.2",
    "react-syntax-highlighter": "^12.2.1",
    "react-table": "^7.1.0",
    "react-tabs": "^3.1.0",
    "react-virtuoso": "^0.17.3",
    "style-it": "^2.1.4",
    "subscriptions-transport-ws": "^0.9.16",
    "typeface-montserrat": "^0.0.75",
    "typeface-roboto-mono": "^0.0.75",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "start": "HOST=dev-admin.kre.local react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prettier": "prettier --write --single-quote \"src/**/*.ts\"",
    "gen:types": "apollo client:codegen types --globalTypesFile=src/Graphql/types/globalTypes.ts --target typescript --includes=./src/Graphql/**/*.graphql --localSchemaFile=../admin-api/schema.graphql && yarn prettier",
    "test:cov": "react-scripts test --coverage --watchAll=false",
    "test:cov:ci": "react-scripts test --coverage --watchAll=false --coverageReporters --testResultsProcessor jest-junit",
    "test:cov:sonar": "react-scripts test --coverage --watchAll=false --coverageReporters --testResultsProcessor jest-sonar-reporter",
    "clear_jest": "jest --clearCache",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/graphql/*/types/*.ts",
      "!<rootDir>/src/graphql/types/globalTypes.ts",
      "!<rootDir>/src/store.ts",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/constants/mock.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 20,
        "statements": 20
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-syntax-highlighter)"
    ]
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@semantic-release/changelog": "3.0.3",
    "@semantic-release/commit-analyzer": "6.1.0",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/github": "7.0.4",
    "@semantic-release/release-notes-generator": "7.1.6",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/chart.js": "^2.9.21",
    "@types/classnames": "^2.2.6",
    "@types/d3-array": "^2.0.0",
    "@types/d3-axis": "^1.0.12",
    "@types/d3-color": "^1.2.2",
    "@types/d3-format": "^1.3.1",
    "@types/d3-interpolate": "^1.3.1",
    "@types/d3-scale": "^2.1.1",
    "@types/d3-selection": "^1.4.1",
    "@types/d3-zoom": "^1.7.4",
    "@types/enzyme": "^3.10.3",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.149",
    "@types/moment-timezone": "^0.5.12",
    "@types/react": "^16.9.3",
    "@types/react-dates": "^17.1.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/react-table": "^7.0.18",
    "apollo": "^2.22.1",
    "conventional-changelog": "3.1.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-sonar": "0.0.7",
    "husky": "^3.0.9",
    "jest-canvas-mock": "^2.1.1",
    "jest-enzyme": "^7.1.1",
    "jest-junit": "10.0.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "semantic-release": "15.9.12",
    "semantic-release-monorepo": "6.1.1",
    "waait": "^1.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "release": {
    "monorepo": {
      "branch": "master",
      "analyzeCommits": {
        "format": "angular",
        "preset": "angular"
      },
      "generateNotes": {
        "preset": "angular",
        "changelogFile": "CHANGELOG.md"
      }
    },
    "branch": "master",
    "verifyConditions": [],
    "prepare": [],
    "publish": [],
    "success": false,
    "fail": false
  }
}

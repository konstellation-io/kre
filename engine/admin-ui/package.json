{
  "name": "admin-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/konstellation-io/kre.git"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.11.2",
    "apollo-upload-client": "17.0.0",
    "axios": "0.24.0",
    "chart.js": "2.9.3",
    "classnames": "2.3.1",
    "d3-array": "3.1.1",
    "d3-axis": "3.0.0",
    "d3-color": "3.0.1",
    "d3-format": "3.1.0",
    "d3-interpolate": "3.0.1",
    "d3-scale": "4.0.2",
    "d3-selection": "3.0.0",
    "d3-zoom": "3.0.0",
    "enzyme-to-json": "3.6.2",
    "graphql": "15.3.0",
    "kwc": "1.15.1",
    "lodash": "4.17.21",
    "lottie-web": "5.8.1",
    "markdown-navbar": "1.4.3",
    "moment": "2.29.1",
    "moment-timezone": "0.5.34",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-grid-resizable": "github:mgs95/react-grid-resizable#build",
    "react-hook-form": "5.7.2",
    "react-hotkeys": "2.0.0",
    "react-markdown": "7.1.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "15.4.5",
    "react-table": "7.7.0",
    "react-toastify": "8.1.0",
    "react-virtuoso": "2.4.0",
    "style-it": "2.1.4",
    "subscriptions-transport-ws": "0.11.0",
    "typeface-montserrat": "1.1.13",
    "typeface-roboto-mono": "1.1.13",
    "typescript": "4.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@types/apollo-upload-client": "14.1.0",
    "@types/chart.js": "2.9.22",
    "@types/classnames": "2.3.1",
    "@types/d3-array": "3.0.2",
    "@types/d3-axis": "3.0.1",
    "@types/d3-color": "3.0.2",
    "@types/d3-format": "3.0.1",
    "@types/d3-interpolate": "3.0.1",
    "@types/d3-scale": "4.0.2",
    "@types/d3-selection": "3.0.2",
    "@types/d3-zoom": "3.0.1",
    "@types/enzyme": "3.10.11",
    "@types/history": "4.7.9",
    "@types/jest": "27.4.0",
    "@types/lodash": "4.14.178",
    "@types/moment-timezone": "0.5.30",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-syntax-highlighter": "13.5.2",
    "@types/react-table": "7.0.27",
    "@welldone-software/why-did-you-render": "6.2.3",
    "apollo": "2.33.9",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint-config-sonar": "0.0.8",
    "husky": "7.0.4",
    "jest-canvas-mock": "2.3.1",
    "jest-enzyme": "7.1.2",
    "jest-junit": "13.0.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "sass": "1.47.0",
    "waait": "1.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "resolutions": {
    "graphql": "15.3.0"
  },
  "scripts": {
    "start": "HOST=dev-admin.kre.local react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prettier": "prettier --write --single-quote \"src/**/*.ts\"",
    "test:cov": "react-scripts test --coverage --watchAll=false",
    "test:cov:sonar": "react-scripts test --silent --env=jsdom --coverage --watchAll=false --testResultsProcessor jest-sonar-reporter",
    "prettier:types": "prettier --write --single-quote \"src/Graphql/**/types/**/*.ts\"",
    "gen:types": "apollo client:codegen types --globalTypesFile=src/Graphql/types/globalTypes.ts --target typescript --includes=./src/Graphql/\"{queries,mutations,subscriptions}\"/*.ts --localSchemaFile=../admin-api/schema.graphql && yarn prettier:types",
    "clear_jest": "jest --clearCache",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/Graphql/**/*.ts",
      "!<rootDir>/src/store.ts",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/Constants/mock.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-syntax-highlighter)"
    ]
  },
  "jestSonar": {
    "reportPath": "./",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
